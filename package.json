{
  "name": "@job-tailor",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --target=build --projects=frontend,backend --parallel=2 --skip-nx-cache",
    "dev": "rm -rf storage && nx run-many --target=serve --projects=frontend,backend --parallel=2 --skip-nx-cache",
    "lint": "nx affected --target=lint",
    "backend:build": "npx nx run backend:build --skip-nx-cache",
    "backend:serve": "rm -rf storage && nx run backend:serve:development --skip-nx-cache",
    "frontend:serve": "npx nx run frontend:serve:development --skip-nx-cache",
    "frontend:build": "npx nx run frontend:build",
    "docker:up": "find docker -name \"docker-compose.yml\" -print0 | xargs -0 -I {} sh -c \"cd \\$(dirname {}) && docker-compose up -d\"",
    "docker:down": "find docker -name \"docker-compose.yml\" -print0 | xargs -0 -I {} sh -c \"cd \\$(dirname {}) && docker-compose down\"",
    "prisma:generate": "prisma generate",
    "prisma:gui": "prisma studio",
    "prisma:migrate": "prisma migrate deploy && nx reset",
    "prisma:push": "prisma db push && nx reset",
    "prisma:seed": "prisma db seed"
  },
  "private": true,
  "engines": {
    "node": ">=20.0.0 <21.0.0"
  },
